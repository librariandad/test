{
    "name": "medlib/textbooks",
    "description": "parses csv textbook file into web display",
    "type": "library",
    "readme": "README.md",
    "license": "MIT",
    "version": "0.9",
    "authors": [
        {
            "name": "Keith Engwall"
        }
    ],
    "require": {
        "php": ">=7.2",
        "league/csv": "^9.6",
        "monolog/monolog": "^2.1",
        "respect/validation": "^2.0",
        "mtdowling/jmespath.php": "^2.6",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.11",
        "filp/whoops": "^2.7"
    },
    "autoload": {
        "psr-4": {"Medlib\\Textbooks\\": "src/textbooks" }
    },
    "autoload-dev": {
        "psr-4": {"Medlib\\Textbooks\\": "tests/textbooks" }
    },
    "config": {
        "bin-dir": "vendor/bin",
        "sort-packages": true
    }

}
